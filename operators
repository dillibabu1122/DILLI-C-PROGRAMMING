#include <stdio.h>
int main()
{   
     int a = 10;
    printf("Value of a is %d\n", a);
    a += 10;
    printf("Value of a is %d\n", a);
     a -= 10;
    printf("Value of a is %d\n", a);
    a*=10;
     printf("Value of a is %d\n", a);
    a /= 10;
     printf("Value of a is %d\n", a);
    a %= 10;
    printf("value of a is %d\n",a);
    return 0;
}

value of a is 10
value of a is 20
value of a is 10
value of a is 100
value of a is 10
value of a is 0
